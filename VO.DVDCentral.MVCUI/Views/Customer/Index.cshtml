@model VO.DVDCentral.BL.Models.Customer

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="form-group">
        <div class="col-md-10">
            @Html.DropDownListFor(model => model.FullName, new SelectList(Model.FullName, "Id", "FullName", Model), null, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.FullName, "", new { @class = "text-danger" })
        </div>
    </div>



